{
  "name": "@llmsalt/llm",
  "version": "0.5.0",
  "description": "Large language models and functionality for Saltcorn",
  "main": "index.js",
  "dependencies": {
    "@saltcorn/data": "^0.9.0",
    "node-fetch": "2.6.9",
    "underscore": "1.13.6",
    "ollama": "0.5.0"
  },
  "author": "Tom Nielsen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/HostMasonOfficial/llmsalt.git"
  },
  "saltcorn": {
    "plugin": true
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "no-unused-vars": "off",
      "no-case-declarations": "off",
      "no-empty": "warn",
      "no-fallthrough": "warn"
    }
  }
}
